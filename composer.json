{
  "name": "liquidrazor/dto-api-bundle",
  "description": "A DTO-first Symfony bundle for building boring JSON APIs with streaming responses and autogenerated OpenAPI documentation.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Noramarth",
      "email": "noramarth@aom.ro"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "symfony/dependency-injection": "^7.0",
    "symfony/config": "^7.0",
    "symfony/http-kernel": "^7",
    "symfony/serializer": "^7",
    "symfony/property-access": "^7.0",
    "symfony/options-resolver": "^7.0",
    "symfony/validator": "^7.0",
    "monolog/monolog": "^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "symfony/browser-kit": "^7",
    "symfony/css-selector": "^7",
    "symfony/twig-bundle": "^7",
    "symfony/twig-bridge": "^7",
    "symfony/web-profiler-bundle": "^7"
  },
  "autoload": {
    "psr-4": {
      "LiquidRazor\\DtoApiBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LiquidRazor\\DtoApiBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "symfony": {
      "bundle": "LiquidRazor\\DtoApiBundle\\DtoApiBundle"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
